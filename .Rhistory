formatted[formatted$Arena == 'Dovercourt', "Latitude"] <- 45.383344
formatted[formatted$Arena == 'Dovercourt', "Longitude"] <- -75.752209
formatted[formatted$Arena == 'Y-Carlingwood', "Latitude"] <- 45.372740
